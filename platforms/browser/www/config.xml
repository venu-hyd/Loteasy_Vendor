<?xml version='1.0' encoding='utf-8'?>
<widget id="com.loteasy.vendor" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Loteasy_Vendor</name>
    <description>
        Loteasy Vendor Application
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Dimenteka Mobile Dev Team
    </author>
    <content src="index.html" />
    <access origin="http://google.com" />
    <access origin="https://google.com" />
    <access origin="http://maps.google.com" />
    <access origin="http://*.google.com" />
    <access origin="content:///*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
</widget>
